{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react",
		"baseUrl": ".",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"resolveJsonModule": true,
		"allowJs": true,
		"noImplicitAny": true,
		"outDir": "dist",
		"sourceMap": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	}
}
